 curl --include \
     --request PUT \
     --header "Content-Type: application/json" \
     --data-binary "{
  \"value\": \"esto es una prueba\"
  
}" \
'http://localhost:1026/v2/entities/myDevice-wastecontainer-sensor-345/attrs/manufacturerName'

